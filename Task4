#include<stdio.h>

int main(){
    const int size = 5;
    int arr[size];
    printf("input number in array:\n");
    for(int i = 0; i < size; ++i){
        scanf("%d",&arr[i]);
    }
    for(int i = 0; i < size; ++i){
        if(arr[i] % 2 == 0){
            printf("%d",arr[i]);
        }
    }
    for(int i = 0; i < size; ++i){
        if(arr[i] % 2 == 1){
            printf("%d",arr[i]);
        }
    }
    return 0;
    
}
