#include<stdio.h>

int main(){
    const int size = 5;
    int arr[size];
    int count = 0;
    printf("input number in array:\n");
    for(int i = 0; i < size; i++){
        scanf("%d",&arr[i]);
        if(arr[i] % 2 == 1){
            count++;
        }
    }
    printf("result: %d\n",count);
    return 0;
}
